    <!-- mdsol extension AuditRecords elements-->
    <xs:attribute name="IncludeFileOID" type="mdsol:YesNoRave"/>
    
    <!-- mdsol extension SiteRef attributes-->
    <xs:attribute name="LocationOIDType" type="mdsol:LocationOIDType"/>
    <xs:attribute name="SiteNumber" type="odm:text"/>
    <xs:attribute name="StudySiteName" type="odm:text"/>
    <xs:attribute name="SiteUUID" type="odm:text"/>
    <xs:attribute name="LocationName" type="odm:text"/>
  
